{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "func-names": "off",
    "global-require": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "max-len": 1
  }
}
